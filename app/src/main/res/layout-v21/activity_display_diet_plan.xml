        <TextView
            android:id="@+id/caloriesbreakfast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="74dp"
            android:layout_marginTop="54dp"
            android:text="TextView" />


        <TextView
            android:id="@+id/calorieslunch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/caloriesbreakfast"
            android:layout_below="@+id/caloriesbreakfast"
            android:layout_marginTop="126dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/caloriesdinner"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/calorieslunch"
            android:layout_below="@+id/calorieslunch"
            android:layout_marginTop="129dp"
            android:text="TextView" />





        <TextView
            android:id="@+id/totalCarbohydrate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="66dp"
            android:layout_marginStart="44dp"
            android:text="TextView" />

        <TextView
            android:id="@+id/totalFat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/totalCarbohydrate"
            android:layout_centerHorizontal="true"
            android:text="TextView" />

        <TextView
            android:id="@+id/totalProtein"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/caloriesdinner"
            android:layout_alignTop="@+id/totalFat"
            android:layout_marginStart="31dp"
            android:text="TextView" />





        <ImageButton
            android:id="@+id/saveplan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/totalCarbohydrate"
            android:layout_below="@+id/totalFat"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="19dp"
            android:layout_marginTop="17dp"
            app:srcCompat="@drawable/dietshowdone_button"
            android:layout_alignEnd="@+id/totalProtein" />
        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/saveplan"
            android:layout_alignStart="@+id/totalFat"
            android:layout_alignTop="@+id/saveplan"
            android:layout_toStartOf="@+id/totalProtein"
            android:paddingRight="28dp"
            android:paddingTop="3dp"
            android:text="Done"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="#ffffff "
            android:textSize="24sp" />



